{
    "hosting": {
      "site": "drincs-website-back-end",
      "public": "build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/app.js",
          "function": "app"
        }
      ]
    }
}
